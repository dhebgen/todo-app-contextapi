{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "noEmit": true,
    "sourceMap": true,
    "noImplicitAny": false,
    "removeComments": true,
    "jsx": "preserve",
    "isolatedModules": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    "src/generated/**"
  ],
  "include": [
    "./src"
  ]
}
